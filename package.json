{
  "name": "list-dependents-cli",
  "version": "2.4.0",
  "description": "Lists all dependents of a project, using npm or ecosyste.ms",
  "homepage": "http://github.com/voxpelli/list-dependents-cli",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/list-dependents-cli.git"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "list-dependents": "cli.js"
  },
  "type": "module",
  "files": [
    "cli.js",
    "lib/**/*.js"
  ],
  "scripts": {
    "check:installed-check": "installed-check -i type-coverage -i @voxpelli/eslint-config -i eslint",
    "check:knip": "knip",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check:type-coverage": "type-coverage --detail --strict --at-least 99 --ignore-files 'test/*'",
    "check": "run-p check:*",
    "prepare": "husky",
    "test": "run-s check"
  },
  "devDependencies": {
    "@types/node": "^20.14.13",
    "@types/semver": "^7.5.8",
    "@voxpelli/eslint-config": "^20.0.0",
    "@voxpelli/tsconfig": "^12.0.1",
    "eslint": "^9.7.0",
    "husky": "^9.0.11",
    "installed-check": "^9.3.0",
    "knip": "^5.26.0",
    "npm-run-all2": "^6.2.0",
    "read-pkg": "^9.0.1",
    "type-coverage": "^2.29.1",
    "typescript": "~5.5.4",
    "validate-conventional-commit": "^1.0.3"
  },
  "dependencies": {
    "buffered-async-iterable": "^1.0.0",
    "bunyan-adaptor": "^6.0.1",
    "chalk": "^5.3.0",
    "list-dependents": "^2.2.0",
    "ora": "^8.0.1",
    "peowly": "^1.3.0",
    "peowly-commands": "^1.0.1",
    "pony-cause": "^2.1.11",
    "semver": "^7.6.2",
    "tempy": "^3.1.0"
  }
}
